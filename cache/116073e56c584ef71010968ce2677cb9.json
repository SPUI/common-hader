,YAAKZ,aAAL,cAAP;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,WAAuCY,CAAC,CAAC,iBAAD,CAAxC,CAD/D,CADF;AAKD;;AACD,aACE,oBAAC,QAAD;AACE,QAAA,OAAO,EAAEmB,IADX;AAEE,QAAA,SAAS,EAAC,aAFZ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAIE;AAAK,QAAA,GAAG,EAAED,MAAM,IAAIjB,OAAO,CAAC,6BAAD,CAA3B;AAA4D,QAAA,GAAG,EAAC,EAAhE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,QAJF,CADF;AAQD,K;;;;;;;6BAEQ;AAAA;;AAAA,UAECL,OAFD,GAEa,KAAKD,KAFlB,CAECC,OAFD;AAIP,aACE;AAAK,QAAA,SAAS,EAAEJ,MAAM,CAACE,SAAvB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SACE;AAAM,QAAA,SAAS,EAAEF,MAAM,CAAC6B,OAAxB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SACE;AACE,QAAA,SAAS,EAAC,eADZ;AAEE,QAAA,OAAO,EAAE,mBAAM;AACb,UAAA,MAAI,CAACnB,QAAL,CAAc;AACZN,YAAAA,OAAO,EAAE,CAACA;AADE,WAAd;AAGD,SANH;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBADF,CADF,EAaE;AAAM,QAAA,SAAS,EAAEJ,MAAM,CAAC8B,QAAxB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SACG,KAAKR,cAAL,EADH,CAbF,EAgBG,KAAKjB,eAAL,EAhBH,CADF;AAoBD;;;;;;;;;;;EAnOqBhB,S;;eAsOTa,S;AAAf;;;;;;;;;;;;;;;;;;;;;;0BAxOMD,O;0BAEAC,S","sourcesContent":["import React, { Component } from 'react';\nimport { Dropdown, Menu, Icon, Drawer } from 'antd';\n\nimport Link from '../components/Link';\n\nimport { MULTICHAIN, ACCOUNTSYSTEM, WALLETSYSTEM } from '../config';\nimport { isCN, getCurrentKey } from '../utils';\n\nimport styles from './styles.module.less';\n\nconst SubMenu = Menu.SubMenu;\n\nclass WapHeader extends Component {\n\n  state = {\n    visible: false,\n  }\n\n  // 渲染导航抽屉\n  renderNavDrawer = () => {\n    const { i18n, t } = this.props;\n    const { visible } = this.state;\n\n    return (\n      <Drawer\n        title={<img src={require('../assets/svg/icon-logo.svg')} alt=\"sparkpool\" />}\n        placement=\"left\"\n        closable={false}\n        visible={visible}\n        onClose={() => {\n          this.setState({\n            visible: false,\n          });\n        }}\n      >\n        <Menu\n          mode=\"inline\"\n          selectedKeys={getCurrentKey()}\n          onClick={({ key }) => {\n            // 切换语言\n            if (['zh', 'en'].indexOf(key) > -1) {\n              i18n.changeLanguage(key);\n              const { pathname, search } = window.location;\n              // /en 路径, 切换中文语言环境\n              if (key === 'zh') {\n                if (pathname.indexOf('/en') > -1) {\n                  window.location.href = pathname.replace('/en', '') + search;\n                }\n              }\n              // / 路径, 切换英文语言环境\n              if (key === 'en') {\n                if (pathname.indexOf('/en') < 0) {\n                  window.location.href = '/en' + pathname + search;\n                }\n              }\n            }\n            this.setState({\n              visible: false,\n            });\n          }}\n        >\n          <Menu.Item key=\"pool\">\n            <a href={MULTICHAIN}>\n              <Icon type=\"home\" />{t('header:pool')}\n            </a>\n          </Menu.Item>\n          <Menu.Item key=\"miner\">\n            <a href=\"/action/mywallet\">\n              <Icon type=\"laptop\" />{t('header:user.myWorkers')}\n            </a>\n          </Menu.Item>\n          <Menu.Item key=\"wallet\">\n            <a href={WALLETSYSTEM}>\n              <Icon type=\"wallet\" />{t('header:wallet')}\n            </a>\n          </Menu.Item>\n          <Menu.Item key=\"download\">\n            <a href={`${MULTICHAIN}/download`}>\n              <Icon type=\"cloud-download\" />{t('header:download')}\n            </a>\n          </Menu.Item>\n          <Menu.Item key=\"staking\">\n            <a href={`https://staking.sparkpool.com`}>\n              <img\n                src={require('../assets/svg/icon-wap-staking.svg')}\n                alt=\"\"\n                style={{\n                  marginRight: 10,\n                  width: 14,\n                }}\n              />Staking\n            </a>\n          </Menu.Item>\n          <SubMenu\n            title={\n              <span>\n                <Icon type=\"question-circle\" />{t('header:help.title')}\n              </span>\n            }\n          >\n            <Menu.Item>\n              <a\n                href={`https://sparkpool.zendesk.com/hc/${isCN() ? 'zh-cn' : 'en-us' }/requests/new`}\n                target=\"_blank\"\n                rel=\"noopener noreferrer\"\n              >\n                {t('header:help.order')}\n              </a>\n            </Menu.Item>\n            <Menu.Item>\n              <a\n                href={`https://support.sparkpool.com/hc/${isCN() ? 'zh-cn' : 'en-us' }/categories/360000149093`}\n                target=\"_blank\"\n                rel=\"noopener noreferrer\"\n              >\n                {t('header:help.question')}\n              </a>\n            </Menu.Item>\n            <Menu.Item>\n              <a\n                href=\"https://mp.weixin.qq.com/mp/homepage?__biz=MzU3MDY4Mjg5Mg==&hid=1&sn=bf68889a560a893a4936fb018b560966\"\n                target=\"_blank\"\n                rel=\"noopener noreferrer\"\n              >\n                {t('header:help.classroom')}\n              </a>\n            </Menu.Item>\n            <Menu.Item>\n              <a\n                href=\"https://mp.weixin.qq.com/mp/homepage?__biz=MzI3OTg3MDQ5NA==&hid=1&sn=9350c778ac3f87dce7bf8b208a4abe21\"\n                target=\"_blank\"\n                rel=\"noopener noreferrer\"\n              >\n                {t('header:help.articles')}\n              </a>\n            </Menu.Item>\n          </SubMenu>\n          <SubMenu\n            title={\n              <span><Icon type=\"global\" />{isCN() ? '简体中文' : 'English'}</span>\n            }\n          >\n            <Menu.Item key=\"zh\">\n              简体中文\n            </Menu.Item>\n            <Menu.Item key=\"en\">\n              English\n            </Menu.Item>\n          </SubMenu>\n        </Menu>\n      </Drawer>\n    )\n  }\n\n  // 渲染用户登陆信息\n  renderUserInfo = () => {\n    const { userInfo, t, logout } = this.props;\n    const { id, avatar } = userInfo || {};\n    const menu = (\n      <div className={styles.dropdown}>\n        <Menu\n          onClick={({ key }) => {\n            if (key === 'logout') {\n              logout();\n            }\n          }}\n        >\n          <Menu.Item key=\"mining\">\n            <Link to=\"/setting/mining\">\n              {t('header:accountmining')}\n            </Link>\n          </Menu.Item>\n          <Menu.Item key=\"follow\">\n            <Link to=\"/setting/follow\">\n              {t('header:user.follow')}\n            </Link>\n          </Menu.Item>\n          <Menu.Item key=\"account\">\n            <Link to=\"/setting/account\">\n              {t('header:user.default')}\n            </Link>\n          </Menu.Item>\n          <Menu.Item key=\"notice\">\n            <Link to=\"/setting/notice\">\n              {t('header:user.notice')}\n            </Link>\n          </Menu.Item>\n          <Menu.Item key=\"userInfo\">\n            <a href={ACCOUNTSYSTEM}>\n              {t('header:user.setting')}\n            </a>\n          </Menu.Item>\n          <Menu.Item key=\"logout\">\n            {t('header:logout')}\n          </Menu.Item>\n        </Menu>\n      </div>\n    );\n    if (!id) {\n      return (\n        <div>\n          <a href={`${ACCOUNTSYSTEM}/login`}>{t('header:login')}</a> / <a href={`${ACCOUNTSYSTEM}/register`}>{t('header:register')}</a>\n        </div>\n      )\n    }\n    return (\n      <Dropdown\n        overlay={menu}\n        placement=\"bottomRight\"\n      >\n        <img src={avatar || require('../assets/svg/icon-user.svg')} alt=\"\"/>\n      </Dropdown>\n    );\n  }\n\n  render() {\n\n    const { visible } = this.state;\n\n    return (\n      <div className={styles.WapHeader}>\n        <span className={styles.navMenu}>\n          <i\n            className=\"icon iconfont\"\n            onClick={() => {\n              this.setState({\n                visible: !visible,\n              });\n            }}\n          >\n            &#xe600;\n          </i>\n        </span>\n        <span className={styles.userMenu}>\n          {this.renderUserInfo()}\n        </span>\n        {this.renderNavDrawer()}\n      </div>\n    );\n  }\n}\n\nexport default WapHeader;\n"]}]}